            <TableColumn fx:id="imageColumn" text="Image" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="quantityColumn" text="Quantity" />
         </columns>
      </TableView>
      <Button fx:id="buttonUnban" layoutX="445.0" layoutY="243.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="62.0" text="Unban" />
      <Label fx:id="labelInfo" alignment="CENTER" layoutY="379.0" prefHeight="17.0" prefWidth="583.0" textAlignment="CENTER">
